URxvt.termName:              rxvt-unicode

! http://vimcolors.com/34/hybrid/light
! black
*color0: #0D0D0D
*color8: #646464
! red
*color1: #5F0000
*color9: #C90808
! green
*color2: #005F00
*color10: #005F00
! yellow
*color3: #8A6307
*color11: #8A6307
! blue
*color4: #1C1C6F
*color12: #1C1C6F
! magenta
*color5: #6D196D
*color13: #6D196D
! cyan
*color6: #207272
*color14: #207272
! white
*color7: #C4C4C4
*color15: #E4E4E4

*font: -*-terminus-medium-*-*-*-32-*-*-*-*-*-*-*
*boldFont: -*-terminus-bold-*-*-*-32-*-*-*-*-*-*-*

urxvt*scrollBar:   false
urxvt*background:  #C4C4C4
urxvt*cursorColor: #646464
urxvt*cursorColor2: #CFCFCF
urxvt*externallBorder:   0
URxvt*internalBorder:    0

! used to be \033. see ascii character control codes
! now this will just enter a ^^ and then the shell will take care of it
! the \033 enters an escape and this is no good for vim in zsh
URxvt.keysym.C-period:  \036
URxvt.keysym.C-semicolon:  \037
