[alias]
  blast = reset --hard HEAD
  fa = fetch --all
  f = fetch
  co = checkout
  st = status
  d = diff
  dc = diff --cached
  dag = log --graph --format='format:%C(yellow)%h%C(reset) %C(blue)%an <%ae>%C(reset) %C(magenta)%cr%C(reset)%C(auto)%d%C(reset)%n%s' --date-order
  ac = commit -am auto
  p = push
  pr = pull --rebase
